<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Dashboard test suite">
    <test name="Number of sections test">
    <classes>
        <class name="selenium.NumberOfSectionsDashboardTest"></class>
    </classes>
    </test>
    <test name="Upper section test">
        <classes>
            <class name="selenium.UpperSectionDashboardTest"></class>
        </classes>
    </test>
    <test name="Sensor list test">
        <classes>
            <class name="selenium.SensorListTest"></class>
        </classes>
    </test>
    <test name="Add point on home plan test">
        <classes>
            <class name="selenium.MapTest"></class>
        </classes>
    </test>
    <test name="Loader test">
        <classes>
            <class name="selenium.LoaderTest"></class>
        </classes>
    </test>
    <test name="Add selected sensor on home plan test">
        <classes>
            <class name="selenium.AddSensorOnHomePlanTest"></class>
        </classes>
    </test>
</suite>